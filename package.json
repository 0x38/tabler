{
  "name": "tabler-ui",
  "description": "Premium and Open Source dashboard template with responsive and high quality UI. For Free!",
  "version": "0.0.32",
  "keywords": [
    "css",
    "sass",
    "mobile-first",
    "responsive",
    "front-end",
    "framework",
    "admin",
    "dashboard",
    "admin panel"
  ],
  "homepage": "https://tabler.github.io",
  "scripts": {
    "gulp-watch": "gulp watch",
    "gulp-build": "gulp build",
    "jekyll-watch": "jekyll build --watch",
    "jekyll-serve": "jekyll serve --livereload",
    "webpack-watch": "webpack --mode='development' --watch",
    "webpack-build": "webpack --mode='production'",
    "gh-pages": "./gh-pages.sh",
    "dist": "gulp build && JEKYLL_ENV=production jekyll build --destination dist",
    "publish-dist": "./gh-pages.sh",
    "watch": "gulp build && npm-run-all --parallel gulp-watch jekyll-watch",
    "serve": "gulp build && npm-run-all --parallel gulp-watch jekyll-serve",
    "js-compile": "npm-run-all --parallel js-compile-*",
    "js-compile-standalone": "rollup --environment bundle:false --config rollup.config.js --sourcemap",
    "js-compile-bundle": "rollup --environment bundle:true --config rollup.config.js --sourcemap",
    "js-minify": "npm-run-all --parallel js-minify-*",
    "js-minify-standalone": "uglifyjs --compress typeofs=false --mangle --comments \"/^!/\" --source-map \"content=js/dist/tabler.js.map,includeSources,url=tabler.min.js.map\" --output js/dist/tabler.min.js js/dist/tabler.js",
    "js-minify-bundle": "uglifyjs --compress typeofs=false --mangle --comments \"/^!/\" --source-map \"content=js/dist/tabler.bundle.js.map,includeSources,url=tabler.bundle.min.js.map\" --output js/dist/tabler.bundle.min.js js/dist/tabler.bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tabler/tabler.git"
  },
  "author": "codecalm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tabler/tabler/issues"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "bootstrap": "^4.1.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-rtlcss": "^1.2.0",
    "gulp-sass": "^3.1.0",
    "jquery": "^3.3.1",
    "npm-run-all": "^4.1.2",
    "rollup": "^0.60.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "uglifyjs": "^2.4.11",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.4"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "browserslist": [
    "last 1 major version",
    ">= 1%",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ]
}
